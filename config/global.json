{
  "webPort": 8080,

  "numChildren": 1,

  "params": {
    "appName": ""
  },

  "sendgrid": {
    "apiUser": "sendgrid_user",
    "apiKey": "sendgrid_key"
  },

  "miner": {
    "enabled": true
  },

  "cors": {
    "origin": true,
    "credentials": true,
    "headers": [ "x-requested-with", "content-type", "X-XSRF-TOKEN" ],
    "methods": [ "GET", "PUT", "POST", "DELETE", "OPTIONS" ],
    "maxAge": 1000000000
  }
}
