{
  "node"        : true,
  "maxerr"      : 100,
  "passfail"    : false,
  "smarttabs"   : false,
  "globals": {
    "console"   : true,
    "process"   : true,
    "spyOn"     : false,
    "it"        : false,
    "describe"  : false,
    "beforeEach": false,
    "afterEach" : false,
    "before"    : false,
    "after"     : false,
    "waits"     : false,
    "waitsFor"  : false,
    "runs"      : false
  },
  "predef": [
    "-Promise"
  ],
  "debug"       : false,
  "devel"       : false,
  "esnext"      : true,
  "strict"      : false,
  "globalstrict": true,
  "asi"         : false,
  "laxbreak"    : false,
  "laxcomma"    : true,
  "bitwise"     : true,
  "boss"        : false,
  "curly"       : true,
  "eqeqeq"      : true,
  "eqnull"      : false,
  "evil"        : false,
  "expr"        : true,
  "forin"       : false,
  "immed"       : true,
  "latedef"     : true,
  "loopfunc"    : false,
  "maxdepth"    : 5,
  "maxparams"   : false,
  "multistr"    : false,
  "noarg"       : true,
  "regexp"      : true,
  "shadow"      : false,
  "supernew"    : false,
  "undef"       : true,
  "unused"      : true,
  "validthis"   : true,
  "quotmark"    : "single",
  "camelcase"   : true,
  "indent"      : 2,
  "newcap"      : true,
  "noempty"     : false,
  "nonew"       : true,
  "nomen"       : false,
  "onevar"      : false,
  "plusplus"    : false,
  "sub"         : false,
  "trailing"    : true,
  "white"       : false
}
